
<!doctype html>

<html lang="en">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.pink-blue.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    <% include ../partials/includes.ejs %>
    <title>Pimenta Rosa - Editar Produto</title>


</head>
<body>

    <div id="vueApp">
        <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
                <% include ../partials/header.ejs %>
            <main class="mdl-layout__content mdl-color--grey-100">
                <div class="mdl-grid">
                    <div class="mdl-grid">
                        
                        <div class="mdl-layout-spacer"></div>
                        <div class="mdl-cell mdl-color--white mdl-shadow--2dp mdl-cell--6-col mdl-grid ">

                            <div class="mdl-cell mdl-cell--12-col">
                                <div class="mdl-typography--headline-color-contrast	mdl-color-text--blue-grey-800">
                                    Editar Produto
                                </div>
                            </div>
                            <!-- Codigo Produto -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <input class="mdl-textfield__input" type="text" v-model="codProduto" id="codProduto">
                                    <label for="missionName" class="mdl-textfield__label">Código</label>
                                </div>
                            </div>
                                
                            <!-- Product Name -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <input class="mdl-textfield__input" type="text" v-model="nomeProduto" id="nomeProduto">
                                    <label for="nomeProduto" class="mdl-textfield__label">Nome</label>
                                </div>
                            </div>

                            <!-- Categoria -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <select class="mdl-textfield__input" id="categoria" name="categoria">
                                        <option></option>
                                        <option class="mdl-menu__item" data-val="PP">Calça</option>
                                        <option class="mdl-menu__item" data-val="P">Camisa</option>
                                        <option class="mdl-menu__item" data-val="M">Vestido</option>
                                        <option class="mdl-menu__item" data-val="G">Sapato</option>
                                    </select>
                                    <label class="mdl-textfield__label" for="categoria">Categoria</label>
                                </div>
                            </div>

                            <!-- Tamanho -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <select class="mdl-textfield__input" id="tamanho" name="tamanho">
                                        <option></option>
                                        <option class="mdl-menu__item" data-val="PP">PP</option>
                                        <option class="mdl-menu__item" data-val="P">P</option>
                                        <option class="mdl-menu__item" data-val="M" data-selected="true">M</option>
                                        <option class="mdl-menu__item" data-val="G">G</option>
                                        <option class="mdl-menu__item" data-val="GG">GG</option>
                                        <option class="mdl-menu__item" data-val="U">U</option>
                                    </select>
                                    <label class="mdl-textfield__label" for="tamanho">Tamanho</label>
                                </div>
                            </div>
                            
                            
                            <!-- Fornecedor -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <select class="mdl-textfield__input" id="fornecedor" name="fornecedor">
                                        <option></option>
                                        <option class="mdl-menu__item" data-val="fornecedor1">fornecedor 1</option>
                                        <option class="mdl-menu__item" data-val="fornecedor2">fornecedor 2</option>
                                        <option class="mdl-menu__item" data-val="fornecedor3">fornecedor 3</option>
                                        <option class="mdl-menu__item" data-val="fornecedor4">fornecedor 4</option>
                                        <option class="mdl-menu__item" data-val="fornecedor5">fornecedor 5</option>
                                        <option class="mdl-menu__item" data-val="fornecedor6">fornecedor 6</option>
                                        
                                    </select>
                                    <label class="mdl-textfield__label" for="fornecedor">Fornecedor</label>
                                </div>
                            </div>
    
                            <!-- Preço pago -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid" id="precoPago">
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <input class="mdl-textfield__input" type="number" v-model="precoPago" pattern="-?[0-9]*(\.[0-9]+)?">
                                    <label class="mdl-textfield__label">Preço pago</label>
                                    <span class="mdl-textfield__error">Input is not a number</span>
                                </div>
                            </div>
                            
                            <!-- Preço venda -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid" id="precoVenda" >
                                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                    <input class="mdl-textfield__input" type="number" v-model="precoVenda" pattern="-?[0-9]*(\.[0-9]+)?">
                                    <label class="mdl-textfield__label">Preço Venda</label>
                                    <span class="mdl-textfield__error">Input is not a number</span>
                                </div>
                            </div>

                            <!-- Description -->
                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                <div class="mdl-textfield mdl-js-textfield mdl-cell--12-col mdl-textfield--floating-label">
                                    <textarea class="mdl-textfield__input" type="text" rows="5" v-model="descricao"></textarea>
                                    <label class="mdl-textfield__label" for="descricao">Descrição...</label>
                                </div>
                            </div>

                            <div class="mdl-cell mdl-cell--12-col mdl-grid">
                                    <button onclick="window.location = '/fornecedores'" style="margin: 8px;" id="button-edit-type" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect">
                                        <span>Cancelar</span>
                                    </button>
                                    <button v-on:click="" style="margin: 8px;" id="button-edit-type" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored">
                                        <span>Atualizar</span>
                                    </button>
                                </div>

                        
                        </div>
                        <div class="mdl-layout-spacer"></div>


                                
                    </div>
                </div>
            </main>
        </div>
	</div>
	

	<script src="https://unpkg.com/vue"></script>
    <script src="https://storage.googleapis.com/code.getmdl.io/1.0.5/material.min.js"></script> <!-- MDL JavaScript -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="../js/material-modal.min.js"></script> 
    <script src="../js/main.js"></script> 
    <script>
		var vueApp = new Vue({
			el: '#vueApp',
			data: {
                products: [
                ],
                name: "",
				evaluators: "",
				experienceGain: "",
                experienceRequire: "",
                tamanho: "",
                fornecedor: "",
                description: "",
				
			},
			methods: {
			
			}
		});
	</script>


</body>
</html> 
